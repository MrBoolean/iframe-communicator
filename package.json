{
  "name": "iframe-communicator",
  "version": "1.0.1",
  "description": "Communicate between two iframes to run scripts synchronously.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest .",
    "build": "cross-env NODE_ENV=development yarn build:clean && yarn build:main && yarn build:minified",
    "build:clean": "del dist/*",
    "build:minified": "babel --minified index.js -o dist/ic.min.js",
    "build:main": "babel index.js -o dist/ic.js",
    "example": "live-server example/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrBoolean/iframe-communicator.git"
  },
  "keywords": [
    "iframe",
    "communication",
    "master",
    "slave",
    "LocalConnection",
    "flash",
    "replacement"
  ],
  "author": "Marc Binder <marcandrebinder@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MrBoolean/iframe-communicator/issues"
  },
  "homepage": "https://github.com/MrBoolean/iframe-communicator#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.22.2",
    "babel-plugin-transform-es2015-modules-umd": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "cross-env": "^3.1.4",
    "del-cli": "^0.2.1",
    "eslint": "^3.13.0",
    "eslint-config-airbnb": "latest",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "jest": "^18.1.0",
    "karma": "^0.13.19",
    "karma-mocha": "^0.2.1",
    "live-server": "^1.2.0",
    "mocha": "^2.3.4"
  },
  "jest": {
    "verbose": true,
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  }
}
